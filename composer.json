{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "darkaonline/l5-swagger": "^8.6",
        "doctrine/dbal": "^3.10",
        "firebase/php-jwt": "^6.11",
        "google/apiclient": "^2.18",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "*",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.29",
        "predis/predis": "^3.2",
        "pusher/pusher-php-server": "^7.2",
        "ramsey/uuid": "^4.9",
        "ratchet/pawl": "^0.4.3",
        "ratchet/rfc6455": "^0.4.0",
        "react/socket": "^1.16"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.87",
        "laravel/dusk": "^7.13",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpstan/phpstan": "^2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Foundation\\": "src/Foundation/",
            "Src\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ssot:guard-baselines": [
            "bash scripts/ci/verify-ssot-baselines.sh"
        ],
        "ssot:lint": [
            "Composer\\Config::disableProcessTimeout",
            "@ssot:guard-baselines",
            "php artisan optimize:clear >/dev/null",
            "bash scripts/ssot/dump_routes.sh",
            "php scripts/ssot/find_orphan_test_routes.php",
            "bash scripts/ssot/lint_tests.sh",
            "@lint:domain-ownership"
        ],
        "lint:domain-ownership": [
            "php scripts/ci/lint-domain-ownership.php"
        ],
        "test:fast": [
            "Composer\\Config::disableProcessTimeout",
            "php -d pcov.enabled=0 ./vendor/bin/phpunit --exclude-group slow,load,stress,redis"
        ],
        "test:slow": [
            "Composer\\Config::disableProcessTimeout",
            "RUN_SLOW_TESTS=1 php -d pcov.enabled=0 ./vendor/bin/phpunit --group slow"
        ],
        "test:load": [
            "Composer\\Config::disableProcessTimeout",
            "RUN_LOAD_TESTS=1 php -d pcov.enabled=0 ./vendor/bin/phpunit --group load"
        ],
        "test:stress": [
            "Composer\\Config::disableProcessTimeout",
            "RUN_STRESS_TESTS=1 php -d pcov.enabled=0 ./vendor/bin/phpunit --group stress"
        ],
        "test:redis": [
            "Composer\\Config::disableProcessTimeout",
            "php -d pcov.enabled=0 ./vendor/bin/phpunit --group redis"
        ],
        "test:nightly": [
            "Composer\\Config::disableProcessTimeout",
            "bash scripts/test/nightly_matrix.sh"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "tymon/jwt-auth"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "discover-packages": false,
        "platform": {
            "php": "8.2.29"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
