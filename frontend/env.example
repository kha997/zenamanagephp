# Environment Configuration for Frontend

# API Configuration
VITE_API_URL=http://localhost:8000/api
VITE_APP_NAME=ZENA Manage
VITE_APP_VERSION=1.0.0

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8000

# Feature Flags
VITE_ENABLE_REAL_TIME=true
