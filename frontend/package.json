{
  "name": "zenamanage-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production tsc && vite build --config vite.config.prod.ts",
    "build:analyze": "NODE_ENV=production tsc && vite build --config vite.config.prod.ts && npx vite-bundle-analyzer dist",
    "lint": "eslint src/components/layout src/shared/auth src/__tests__/filterMenu.test.ts e2e --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src/components/layout src/shared/auth src/__tests__/filterMenu.test.ts e2e --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "npx vite build && playwright test",
    "test:e2e:ui": "playwright test --ui",
    "preview": "vite preview",
    "preview:prod": "vite preview --config vite.config.prod.ts",
    "clean": "rm -rf dist node_modules/.vite",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "size": "npm run build && npx bundlephobia",
    "lighthouse": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "perf": "npm run build && npm run preview:prod & sleep 5 && npm run lighthouse",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@tanstack/react-query": "^5.8.4",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dropzone": "^4.2.2",
    "axios": "^1.6.2",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "emoji-picker-react": "^4.5.16",
    "framer-motion": "^12.23.16",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "laravel-echo": "^2.2.6",
    "lucide-react": "^0.294.0",
    "pusher-js": "^8.4.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.1.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.25.76",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@playwright/test": "^1.56.0",
    "@storybook/addon-a11y": "^9.1.15",
    "@storybook/addon-docs": "^9.1.15",
    "@storybook/addon-onboarding": "^9.1.15",
    "@storybook/addon-vitest": "^9.1.15",
    "@storybook/react-vite": "^9.1.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^9.1.15",
    "history": "^5.3.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.31",
    "storybook": "^9.1.15",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
