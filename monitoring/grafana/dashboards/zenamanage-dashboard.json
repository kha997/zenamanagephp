{
  "dashboard": {
    "id": null,
    "title": "ZenaManage Performance Dashboard",
    "tags": ["zenamanage", "performance"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Application Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "zenamanage_active_users",
            "legendFormat": "Active Users"
          },
          {
            "expr": "zenamanage_requests_per_minute",
            "legendFormat": "Requests/min"
          },
          {
            "expr": "zenamanage_error_rate",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "basic"
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "zenamanage_response_time_avg",
            "legendFormat": "Average Response Time (ms)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none"
            },
            "unit": "ms"
          }
        }
      },
      {
        "id": 3,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "zenamanage_cpu_usage",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "zenamanage_memory_usage",
            "legendFormat": "Memory Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none"
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 4,
        "title": "Database Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "zenamanage_db_connections",
            "legendFormat": "DB Connections"
          },
          {
            "expr": "zenamanage_slow_queries",
            "legendFormat": "Slow Queries"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Error Rate Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "zenamanage_error_rate",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "none"
            },
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}
