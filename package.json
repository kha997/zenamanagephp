{
  "name": "zenamanage",
  "version": "1.0.0",
  "description": "ZenaManage - Multi-tenant Project Management System",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dedupe:check": "jscpd --min-lines 10 --min-tokens 50 --threshold 5 --reporters console,html --output ./reports/jscpd resources/js src/",
    "dedupe:php": "php -l app/Http/Controllers/*.php && php -l app/Http/Middleware/*.php && php -l app/Http/Requests/*.php",
    "lint:sonar": "eslint resources/js src/ --ext .js,.ts,.jsx,.tsx --config .eslintrc.sonarjs.js",
    "lint:fix": "eslint resources/js src/ --ext .js,.ts,.jsx,.tsx --fix",
    "css:sort": "tailwindcss --input resources/css/app.css --output public/css/app.css --watch",
    "ci:check": "npm run dedupe:check && npm run dedupe:php && npm run lint:sonar",
    "test": "php artisan test",
    "test:coverage": "php artisan test --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:auth": "playwright test --config=playwright.auth.config.ts",
    "test:auth:headed": "playwright test --config=playwright.auth.config.ts --headed",
    "test:auth:ui": "playwright test --config=playwright.auth.config.ts --ui",
    "test:auth:report": "playwright show-report test-results/auth/html-report",
    "test:e2e:smoke": "playwright test --project=smoke-chromium",
    "test:e2e:smoke:headed": "playwright test --project=smoke-chromium --headed",
    "test:e2e:report": "playwright show-report",
    "test:smoke": "playwright test --project=smoke-chromium",
    "test:smoke:mobile": "playwright test --project=smoke-mobile",
    "test:smoke:all": "playwright test --project=smoke-chromium --project=smoke-mobile",
    "test:smoke:ui": "playwright test --project=smoke-chromium --ui",
    "test:smoke:headed": "playwright test --project=smoke-chromium --headed",
    "test:smoke:debug": "playwright test --project=smoke-chromium --debug",
    "test:core": "playwright test --project=core-chromium",
    "test:core:all": "playwright test --project=core-chromium --project=core-firefox --project=core-webkit",
    "test:regression": "playwright test --project=regression-chromium",
    "test:regression:all": "playwright test --project=regression-chromium --project=regression-mobile",
    "analyze:duplicates": "npm run dedupe:check && npm run dedupe:php",
    "analyze:quality": "npm run lint:sonar",
    "analyze:all": "npm run analyze:duplicates && npm run analyze:quality",
    "pre-commit": "npm run ci:check",
    "lint": "eslint src/components/ui/header --ext .ts,.tsx",
    "format": "prettier --write \"./{app,resources,src,frontend,docs,tests}/**/*.{js,ts,tsx,jsx,css,scss,md,php,blade.php,json,mdx}\"",
    "format:check": "prettier --check \"./{app,resources,src,frontend,docs,tests}/**/*.{js,ts,tsx,jsx,css,scss,md,php,blade.php,json,mdx}\"",
    "check:ui-standards": "node scripts/ui-standards/check-ui-standards.js",
    "check:performance": "node scripts/ui-standards/check-performance-budgets.js",
    "storybook:build": "node scripts/ui-standards/ensure-storybook-build.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jscpd": "^3.5.4",
    "jsdom": "^27.0.1",
    "laravel-vite-plugin": "^2.0.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vitest": "^4.0.1",
    "prettier": "^3.3.3",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "ts-morph": "^22.0.0"
  },
  "dependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "alpinejs": "^3.13.3",
    "axios": "^1.12.2",
    "chart.js": "^4.4.1",
    "laravel-echo": "^2.2.4",
    "lodash": "^4.17.21",
    "pusher-js": "^8.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}